							$.getJSON('/scitech/search.singlepage/?osti_id=1122130&q='+escape(docterms), function(data) {
								$.each(data.highlighting, function(idx, value) {
									docresults[idx] = {index: idx+1, osti_id:data.response.docs[idx].osti_id, page_number:data.response.docs[idx].page_number, content:value.content};
								});
								$('#citation-searchresults-numpages').text(data.highlighting.length);
								$('#docsearch-results').html( Handlebars.templates.docsearch_result(docresults) ).css("counter-reset", "item"); // the css is to work around an IE bug that fails to increment the results
							});
						}
					}
					function docSearchPreview(prevId) {
						var thisPrev = $('#'+prevId);
						$('#docsearch-preview_label').text(thisPrev.attr('data-title'));
						$('#docsearch-preview_body').html('<img src="'+ thisPrev.attr('data-href') +'" border="0"/>');
						var pvnum = thisPrev.attr("id").replace("ds-preview-","") * 1;
						var pvpgr = '';
						if (pvnum==1){
							pvpgr = pvpgr + '<a href="#" class="btn btn-sm btn-default disabled" onclick="return false;">&laquo; Prev</a>';
						} else {
							pvpgr = pvpgr + '<a href="#" class="btn btn-sm btn-default" onclick="docSearchPreview(\'ds-preview-'+ (pvnum-1) +'\');return false;">&laquo; Prev</a>';
						}
						if ($('#ds-preview-'+(pvnum+1)).attr("id") == null){
							pvpgr = pvpgr + '<a href="#" class="btn btn-sm btn-default disabled" onclick="return false;">Next &raquo;</a>';
						} else {
							pvpgr = pvpgr + '<a href="#" class="btn btn-sm btn-default" onclick="docSearchPreview(\'ds-preview-'+ (pvnum+1) +'\');return false;">Next &raquo;</a>';
						}
						$('#docsearch-preview_pager').html(pvpgr);
					}
				</script>
				<script type="text/javascript">
